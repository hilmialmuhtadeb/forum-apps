{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "cypress/globals": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "cypress"
    ],
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "import/prefer-default-export": 0,
        "react/forbid-prop-types": 0,
        "linebreak-style": 0,
        "no-param-reassign": 0,
        "no-alert": 0,
        "jsx-a11y/label-has-associated-control": 0
    }
}
